{
    "openapi": "3.0.0",
    "info": {
        "title": "BRAMON API",
        "contact": {
            "name": "Support Team",
            "email": "support@bramonmeteor.org"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/v1/public/stations": {
            "get": {
                "tags": [
                    "get-public-stations"
                ],
                "summary": "List all stations",
                "operationId": "/v1/public/stations",
                "responses": {
                    "200": {
                        "description": "List all stations",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Error: Bad request. When required parameters were not supplied."
                    }
                }
            }
        },
        "/v1/public/stations/{station}": {
            "get": {
                "tags": [
                    "get-public-stations-single"
                ],
                "summary": "View a station",
                "operationId": "/v1/public/stations/000-000-0000",
                "parameters": [
                    {
                        "name": "station",
                        "in": "path",
                        "description": "The station identifier",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns some sample category things",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "404": {
                        "description": "Error: Not Found. When station not exists or not public."
                    }
                }
            }
        }
    }
}